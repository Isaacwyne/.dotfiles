configuration {
    display-drun: "Applications:";
    display-window: "Windows:";
    drun-display-format: "{icon} {name}";
    font: "JetBrainsMonoMedium Nerd Font 10";
    modi: "window,run,drun,emoji";
    show-icons: true;
    icon-theme: "Win11-Dark";
}

window {
    border: 1px;
    border-color: #5e81ac;
}

@theme "app_launcher.rasi"
